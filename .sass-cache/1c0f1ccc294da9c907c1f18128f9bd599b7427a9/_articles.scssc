.article-1 {
	background-color: $a1-light-purple;
	position: relative;

	&:before {
		content: '';
		background-color: $a1-light-purple;
		height: 100%;
		width: 100%;
		display: block;
		left: -100%;
		position: absolute;
	}

	&:after {
		content: '';
		background-color: $a1-light-purple;
		height: 100%;
		width: 22px;
		display: block;
		right: -22px;
		position: absolute;
		top: 0;
	}

	h1 {
		color: $white;
		font-family: $font-lato;
		font-weight: 100;
		@include rem(font-size, 279px);
		@include rem(line-height, 220px);
		position: absolute;
		z-index: 4;
		margin-top: 40px;
		left: -20px;
	}

	.inner {
		min-height: 310px;
		text-align: center;
		margin-top: 700px;
		padding: 0 20px;
		position: relative;
		background: rgb(217,213,229); /* Old browsers */
		background: -moz-linear-gradient(top,  rgba(217,213,229,1) 0%, rgba(207,203,222,1) 100%); /* FF3.6+ */
		background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(217,213,229,1)), color-stop(100%,rgba(207,203,222,1))); /* Chrome,Safari4+ */
		background: -webkit-linear-gradient(top,  rgba(217,213,229,1) 0%,rgba(207,203,222,1) 100%); /* Chrome10+,Safari5.1+ */
		background: -o-linear-gradient(top,  rgba(217,213,229,1) 0%,rgba(207,203,222,1) 100%); /* Opera 11.10+ */
		background: -ms-linear-gradient(top,  rgba(217,213,229,1) 0%,rgba(207,203,222,1) 100%); /* IE10+ */
		background: linear-gradient(to bottom,  rgba(217,213,229,1) 0%,rgba(207,203,222,1) 100%); /* W3C */
		filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#d9d5e5', endColorstr='#cfcbde',GradientType=0 ); /* IE6-9 */

		&:before {
			content: '';
			background: rgb(217,213,229); /* Old browsers */
			background: -moz-linear-gradient(top,  rgba(217,213,229,1) 0%, rgba(207,203,222,1) 100%); /* FF3.6+ */
			background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(217,213,229,1)), color-stop(100%,rgba(207,203,222,1))); /* Chrome,Safari4+ */
			background: -webkit-linear-gradient(top,  rgba(217,213,229,1) 0%,rgba(207,203,222,1) 100%); /* Chrome10+,Safari5.1+ */
			background: -o-linear-gradient(top,  rgba(217,213,229,1) 0%,rgba(207,203,222,1) 100%); /* Opera 11.10+ */
			background: -ms-linear-gradient(top,  rgba(217,213,229,1) 0%,rgba(207,203,222,1) 100%); /* IE10+ */
			background: linear-gradient(to bottom,  rgba(217,213,229,1) 0%,rgba(207,203,222,1) 100%); /* W3C */
			filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#d9d5e5', endColorstr='#cfcbde',GradientType=0 ); /* IE6-9 */
			height: 100%;
			width: 100%;
			display: block;
			left: -100%;
			position: absolute;
		}

		&:after {
			content: '';
			background: rgb(217,213,229); /* Old browsers */
			background: -moz-linear-gradient(top,  rgba(217,213,229,1) 0%, rgba(207,203,222,1) 100%); /* FF3.6+ */
			background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(217,213,229,1)), color-stop(100%,rgba(207,203,222,1))); /* Chrome,Safari4+ */
			background: -webkit-linear-gradient(top,  rgba(217,213,229,1) 0%,rgba(207,203,222,1) 100%); /* Chrome10+,Safari5.1+ */
			background: -o-linear-gradient(top,  rgba(217,213,229,1) 0%,rgba(207,203,222,1) 100%); /* Opera 11.10+ */
			background: -ms-linear-gradient(top,  rgba(217,213,229,1) 0%,rgba(207,203,222,1) 100%); /* IE10+ */
			background: linear-gradient(to bottom,  rgba(217,213,229,1) 0%,rgba(207,203,222,1) 100%); /* W3C */
			filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#d9d5e5', endColorstr='#cfcbde',GradientType=0 ); /* IE6-9 */
			height: 100%;
			width: 22px;
			display: block;
			right: -22px;
			position: absolute;
			top: 0;
			z-index: 1;
		}

		img {
			position: absolute;
			top: -250px;
			left: -45px;
			z-index: 5;
		}

		.body {
			position: absolute;
			top: 85px;

			h3 {
				font-family: $font-lora;
				@include rem(font-size, 36px);
				font-weight: 700;
				font-style: italic;
				color: $a1-purple-heading;
				margin-bottom: $spacing;
			}

			p {
				font-family: $font-lato;
				color: $a1-purple-body;
				@include rem(font-size, 19px);
				@include rem(line-height, 27px);
			}

		}		
	}
}

.article-2 {
	padding: 140px 0 40px 50px;
	position: relative;

	.inner {
		position: relative;
		min-height: 571px;

		img {
			position: absolute;
			top: 0;
			left: -185px;
			z-index: 1;
		}

		.hgroup {
			position: absolute;
			z-index: 2;
			top: 370px;
		}

		h6, h2 {
			text-transform: uppercase;
		}

		h6 {
			font-family: $font-merriweather;
			@include rem(font-size, 13px);
			color: $a2-beige;
		}

		h2 {
			font-family: $font-oswald;
			@include rem(font-size, 75px);
			@include rem(line-height, 79px);
			color: $a2-mustard;
			margin-bottom: 28px;
		}
	}

	h5 {
		font-family: $font-lora;
		@include rem(font-size, 12px);
		font-style: italic;
		text-transform: uppercase;
		color: $a2-purple;
		text-align: right;
	}

	p {
		font-family: $font-merriweather;
		font-weight: 300;
		@include rem(font-size, 25px);
		@include rem(line-height, 33px);
	}
}

.article-3 {
	text-align: right;
	padding: 0 18px 195px;
	position: relative;

	h4 {
		font-family: $font-oswald;
		@include rem(font-size, 21px);
		color: $a3-green;
		margin-bottom: 12px;
	}

	p {
		font-family: $font-lato;
		@include rem(font-size, 14px);
		@include rem(line-height, 19px);
		color: $a3-grey;
	}

	.wellington {
		position: absolute;
		left: 7px;
		bottom: -20px;
	}
}

.article-4 {

	h4 {
		border-style: solid;
		border-width: 1px 0;
		border-color: $a4-grey-rule;
		font-family: $font-oswald;
		@include rem(font-size, 15px);
		text-transform: uppercase;
		color: $a4-gold;
		text-align: center;
		padding: 16px 0;
		margin-bottom: 15px;

		span {
			font-family: $font-lato;
			@include rem(font-size, 12px);
			color: $a4-cream;
		}
	}

	.bullet-list {
		@extend .clearfix;

		li {
			border-right: 1px solid $a4-grey-rule;
			width: 20%;
			@include box-sizing(border-box);
			float: left;
			position: relative;
			padding-bottom: 30px;

			img {
				position: absolute;
				left: 26%;
			}

			&:last-child {
				border: none;
			}

			&.remington, &.winchester {
				padding-top: 250px;
				@include rem(font-size, 28px);
				@include rem(line-height, 34px);

				.large {
					@include rem(font-size, 32px);
					@include rem(line-height, 35px);
				}

				.xlarge {
					@include rem(font-size, 79px);
				}

				img {

				}
			}

			&.remington {

				img {
					top: 50px;
				}
			}

			&.winchester {

				img {
					top: 5px;
				}
			}

			&.russian {
				padding-top: 250px;
				@include rem(font-size, 31px);
				@include rem(line-height, 45px);

				.large {
					@include rem(font-size, 39px);
					@include rem(line-height, 40px);
				}

				.xlarge {
					@include rem(font-size, 40px);
					@include rem(line-height, 44px);
				}

				img {
					top: 60px;
				}
			}

			&.thumper-220 {
				padding-top: 239px;
				@include rem(font-size, 38px);
				@include rem(line-height, 41px);

				.large {
					@include rem(font-size, 40px);
				}

				.xlarge {
					@include rem(font-size, 87px);
					@include rem(line-height, 42px);
				}

				img {
					top: 60px;
				}
			}

			&.thumper-240 {
				padding-top: 239px;
				@include rem(font-size, 32px);
				@include rem(line-height, 39px);

				.large {
					@include rem(font-size, 38px);
					@include rem(line-height, 43px);
				}

				.xlarge {
					@include rem(font-size, 87px);
					@include rem(line-height, 42px);
				}

				img {
					top: 50px;
				}
			}

			p {
				color: $a4-grey-rule;
				font-family: $font-oswald;
				text-transform: uppercase;
				text-align: center;

				.lower {
					text-transform: lowercase;
				}
			}
		}
	}
}

.article-5 {
	color: $white;

	h3 {
		@include rem(font-size, 49px);
		@include rem(line-height, 79px);
		font-family: $font-oswald;
		text-transform: uppercase;
		background-color: $a5-brown;
		text-align: center;
	}

	figure {
		@include rem(font-size, 26px);
		@include rem(line-height, 35px);
		font-family: $font-merriweather;
		font-weight: 300;
		background-color: $a5-light-brown;

		figcaption {
			padding: 22px 22px 30px;
		}

		.resource {
			display: block;
			@include rem(font-size, 18px);
			color: $a5-grey;
			margin-top: 35px;

			&:hover, &:focus {
				color: $white;
			}
		}
	}
}

.article-6 {
	background: transparent url('../images/immigration.jpg') 0 0 no-repeat;
	padding-top: 170px;

	h3, h4 {
		font-family: $font-oswald;
		@include rem(line-height, 79px);
	}

	h3 {
		text-transform: uppercase;
		@include rem(font-size, 82px);
		padding-bottom: 20px;
		border-bottom: 20px solid $black;
	}

	h4 {
		@include rem(font-size, 39px);
		@include rem(line-height, 79px);
		border-bottom: 3px solid $black;
		margin-bottom: 16px;
	}

	ul {
		@include rem(font-size, 22px);
		@include rem(line-height, 29px);
		font-family: $font-merriweather;
		font-weight: 300;

		li {
			display: inline;	
		}

		b {
			font-weight: 900;
		}
	}
}

.article-7 {
	padding: 13px 0 0 13px;
	
	.inner {
		background-color: $a7-blue;
		color: $white;
		padding-bottom: 28px;
	}

	.pad {
		padding: 0 12px;
	}

	h4 {
		@include rem(font-size, 21px);
		@include rem(line-height, 30px);
		font-family: $font-oswald;
		text-transform: uppercase;
		padding-bottom: 15px;
		border-bottom: 10px solid $a7-light-blue;
		margin-bottom: 15px;

		.large {
			@include rem(font-size, 31px);
		}
	}

	.overview {
		@include rem(font-size, 16px);
		@include rem(line-height, 24px);
		font-family: $font-lora;
		font-weight: 400;
		font-style: italic;
		margin-bottom: 24px;
	}

	.hr-dash-top {
		border-style: dashed none solid;
		border-color: $a7-dark-blue transparent $a7-mid-blue;
		border-width: 2px 0 5px;
	}

	.hr-solid-top {
		border-style: solid none dashed;
		border-color: $a7-mid-blue transparent $a7-dark-blue;
		border-width: 5px 0 2px;
	}

	.rotten {
		@include rem(font-size, 14px);
		@include rem(line-height, 30px);
		font-family: $font-oswald;
		text-transform: uppercase;
		color: $a7-light-blue;

		.rating {
			@include rem(font-size, 32px);
			display: block;
		}

		.percentage {
			@include rem(font-size, 54px);
			color: $white;
		}
	}

	.event {
		font-family: $font-oswald;
		text-transform: uppercase;
		text-align: center;

		h5 {
			color: $a7-light-blue;
			@include rem(font-size, 14px);
			@include rem(line-height, 32px);
		}

		.p-name {
			@include rem(font-size, 27px);
			@include rem(line-height, 33px);
			color: $white;
		}

		.p-country-name {
			@include rem(font-size, 28px);
			@include rem(line-height, 34px);
			color: $a7-light-blue;
			display: block;
		}

		.dt-start {
			@include rem(font-size, 39px);
			@include rem(line-height, 43px);
			color: $white;
		}
	}
}

.article-8 {
	background: transparent url('../images/overflow-bkg.png') 0 0 no-repeat;
	padding-top: 92px;
	margin-bottom: 60px;

	h3 {
		font-family: $font-oswald;
		text-transform: uppercase;
		@include rem(font-size, 82px);
		@include rem(line-height, 79px);
		color: $a8-blue;
		@include transform(6deg);
		margin-left: 62px;
		margin-bottom: 10px;
	}

	p {
		font-family: $font-lora;
		@include rem(font-size, 16px);
		@include rem(line-height, 26px);
		font-style: italic;
		color: $a8-grey;
		margin-bottom: 15px;

		b {
			font-weight: 700;
		}

		&.para-1 {
			text-align: right;
			@include transform(2deg);
			width: 77%;
			float: right;
		}

		&.para-2 {
			clear: right;
			@include transform(-1deg);
			width: 70%;
			margin-left: 28px;
		}

		&.para-3 {
			clear: right;
			@include transform(1deg);
			width: 80%;
			margin-left: 145px;
		}
	}
}

.footer {
	border-top: 1px solid $footer-grey-rule;
	padding: 20px 0 60px;

	small {
		font-family: $font-lato;
		@include rem(font-size, 17px);
		@include rem(line-height, 26px);
		color: $footer-grey;
	}

	.highlight, .p-name {
		color: $footer-dark-grey;
	}

	small {
		float: left;
	}

	.h-card {
		float: right;
		text-align: right;
		font-family: $font-merriweather;
		@include rem(font-size, 13px);
		@include rem(line-height, 19px);
		color: $footer-grey;
	}
